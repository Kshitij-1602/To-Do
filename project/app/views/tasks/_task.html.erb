<div id="<%= dom_id task %>">
  <p>
    <strong>Task name:</strong>
    <%= task.task_name %>
  </p>

  <p>
    <strong>Date time:</strong>
    <%= task.date_time %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= task.description %>
  </p>

  <p>
    <strong>State:</strong>
    <%= task.state %>
  </p>

  <p>
    <strong>Comments:</strong>
    <%= task.comments %>
  </p>

  <p>
    <strong>User:</strong>
    <%= task.user_id %>
  </p>

</div>
